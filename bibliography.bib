	journal = {Nuclear Engineering and Design},
	author = {Altahhan, Muhammad Ramzy and Bhaskar, Sandesh and Ziyad, Devshibhai and Balestra, Paolo and Fiorina, Carlo and Hou, Jason and Smith, Nicholas and Avramova, Maria},
	month = dec,
