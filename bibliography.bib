@inproceedings{rineiski_kinetics_2005,
	address = {Palais des Papes, Avignon, France},
	title = {Kinetics and cross-section developments for analyses of reactor transmutation concepts with {SIMMER}},
	abstract = {SIMMER is a coupled (neutronics, thermal-hydraulics, structure) code for nuclear reactor safety studies. The paper is devoted to recent neutronics, in particular kinetics and cross-section preparation developments, which were performed at FZK to extend the code capabilities to analyses of reactor transmutation concepts. In the first part of the paper, a spatial kinetics model based on the improved quasistatic scheme, which takes into account an external neutron source and movement of delayed neutron precursors, is presented. For flux shape recalculations, the precursor term is split into two terms: a "standard" term, which is computed in the neutronics part of the code similar to the solid fuel case, and a "movable" term that is computed in the thermal-hydraulics part of the code on the basis of the "standard" term. The space-time factorization is applied to the flux and to the "standard" term, but a weighting procedure is employed for computing the effective "movable" term in the point-kinetics part of the quasistatic scheme. The latter term is treated as an additional "effective source". At steady-state conditions in a critical system with movable fuel, the reactivity is positive and the "effective source" is negative, just opposite to a system with an external neutron source and solid fuel. The reactivity and effective delayed neutron fraction are computed independently upon the fuel flow rate during the transient. In the second part of the paper, a new code and data system for generating multigroup cross-section libraries for SIMMER is described. The system is based on a fine-group "master" library in the extended (to take properly into account neutron scattering at low energies) CCCC (ISOTXS and BRKOXS) format. The fine energy group structure is chosen so that this library and corresponding point-wise cross-sections give quite similar criticality values for a wide range of simplified reactor models, which can be potentially used for nuclear waste burning. Problem-oriented libraries in the extended CCCC format with a smaller number of groups and nuclides (a "steel" nuclide can be employed instead of Fe, Cr, and Ni isotopes) can be generated on the basis of the master library. It is shown that for a molten salt system without graphite moderator, the upper boundary of the energy region, in which the neutron upscattering treatment is important, is higher than in conventional light-water reactors.},
	booktitle = {Mathematics and {Computation}, {Supercomputing}, {Reactor} {Physics} and {Nuclear} and {Biological} {Applications}},
	publisher = {American Nuclear Society},
	author = {Rineiski, A and Sinitsa, Valentin and Maschek, W. and Wang, Shisheng},
	month = sep,
	year = {2005},
}
